    "has-binary": "0.1.7",
    "hat": "^0.0.3",
    "simple-peer": "^7.0.0",
    "socket.io-p2p-parser": "tomcartwrightuk/socket.io-p2p-parser",
    "to-array": "^0.1.4",
    "webrtcsupport": "^2.2.0"
  },
  "devDependencies": {
    "concat-stream": "^1.6.0",
    "ecstatic": "0.8.0",
    "envify": "^3.4.0",
    "expect": "^1.1.0",
    "expect.js": "^0.3.1",
    "inherits": "~2.0.1",
    "simple-get": "^1.4.3",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "socket.io-p2p-server": "^1.2.0",
    "standard": "^10.0.0",
    "tape": "^4.6.3",
    "xhr": "^2.4.0",
    "zuul": "^3.11.1"
